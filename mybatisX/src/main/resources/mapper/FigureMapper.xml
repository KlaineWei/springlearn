<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisx.mapper.FigureMapper">

    <resultMap id="BaseResultMap" type="com.example.mybatisx.entity.Figure">
            <id property="id" column="id" jdbcType="OTHER"/>
            <result property="height" column="height" jdbcType="DOUBLE"/>
            <result property="weight" column="weight" jdbcType="DOUBLE"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,height,weight
    </sql>
</mapper>
